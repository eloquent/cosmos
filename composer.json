        "namespace",
        "class",
        "function",
        "name",
        "classname",
        "use",
        "statement",
        "resolver",
        "solver"
    ],
    "homepage": "https://github.com/eloquent/cosmos",
    "license": "MIT",
    "authors": [
        {
